<?xml version="1.0" encoding="utf-8"?>
<resources>
    <array name="dataHelp">
        <item>量词</item>
        <item>. 任意字符</item>
        <item>* 零次或更多</item>
        <item>+ 一次或更多</item>
        <item>\? 零次或一次</item>
        <item>{n} n 次</item>
        <item>{n,} n 次或更多</item>
        <item>{n,m} n 到 m 次</item>
        <item>字符</item>
        <item>\d 数字</item>
        <item>\D 非数字</item>
        <item>\w 字母</item>
        <item>\W 非字母</item>
        <item>\s 空白字符</item>
        <item>\S 非空白字符</item>
        <item>定位符</item>
        <item>^ 行首</item>
        <item>$ 行尾</item>
        <item>转义字符 (\)</item>
        <item>([{{\^-=$!</item>
        <item>|]}})?*+.\</item>
        <item>组和范围</item>
        <item>[abc] 范围 (a, b 或 c)</item>
        <item>[^abc] 非 a, b 或 c</item>
        <item>[a-z] 从 a 到 z</item>
        <item>[1-9] 从 1 到 9</item>
        <item>(abc) 组 abc</item>
        <item>x|y 字符 x 或 y</item>
        <item>(?:abc) 非捕获组</item>
        <item>(?=abc) 正向预查</item>
        <item>(?!abc) 负向预查</item>
        <item>(?&lt;=abc) 正向后发</item>
        <item>(?&lt;!abc) 负向后发</item>
    </array>

